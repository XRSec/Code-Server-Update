#     && mv /www/bak/ssl_cert/ca.key /www/bak/ssl_cert/vscode.key

RUN yum -y install python2 python3.9 \
    && yum clean all

EXPOSE 22 5050 5051 8765 31000 31001 31002 31003

ENV TZ='Asia/Shanghai'
ENV LANG 'zh_CN.UTF-8'

STOPSIGNAL SIGQUIT

CMD ["/code-server.sh"]
